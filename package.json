{
  "name": "econify_challenge",
  "version": "1.5.0",
  "description": "Basic GraphQL Server made with TypeScript",
  "scripts": {
    "build": "tsc",
    "lint": "npx eslint --config .eslintrc.js 'src/**/*.ts' --max-warnings 1",
    "precommit": "npm run typecheck && npm run lint && npm run test:ci",
    "start": "nodemon",
    "test": "npx jest --watch",
    "test:ci": "npx jest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.1.16",
    "apollo-datasource": "^3.1.0",
    "apollo-server": "^3.3.0",
    "dotenv": "^10.0.0",
    "graphql": "^15.5.3",
    "graphql-tools": "^8.2.0",
    "sequelize": "^6.6.5",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@tsconfig/node12": "^1.0.9",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^10.17.60",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "babel-jest": "^27.2.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.1",
    "node-fetch": "^3.0.0",
    "nodemon": "^2.0.13",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__utils"
    ]
  }
}
